% find route
laser "findroute startx=3.3 starty=0.3 endx=0.38 endy=3.38"
wait 1
%%%%%%%%%%%%%
%% Kalman localization
laser "localize"
laser "push t='1.0' cmd='localize'"
wait 5
%% set odometry
%set "$odox" 3.3
%set "$odoy" 0.35
%set "$odoth" 0
%% run route
c = $l4
eval c
label "loop"
stringcat "getpoint p="c
laser "$string"
wait 1
eval $l5
eval $l6
eval $l7
th = $l7/3.14159*180
drivew $l5 $l6 th:($targetdist<0.25)
wait 3
c=c-1
if (c>-1) "loop"